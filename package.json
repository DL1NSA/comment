{
    "name": "comment-vscode",
    "displayName": "comment-vscode",
    "description": "Enjoy Section Comments of KFCS that you used to generate with Comment IV inside your Visual Studio Code.",
    "version": "0.0.1",
    "publisher": "karyfoundation",
    "engines": {
        "vscode": "^0.10.1"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:commentVSCode.makeLineSectionComment"
    ],
    "main": "./comment",
    "contributes": {
        "keybindings": [{
            "command": "commentVSCode.makeLineSectionComment",
            "key": "ctrl+y",
            "mac": "cmd+y",
            "when": "editorTextFocus"
        }]
    },
    "devDependencies": {
        "vscode": "0.10.x"
    }
}